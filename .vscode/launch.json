    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "gen",
            "type": "python",
            "request": "launch",
            "program": "synthtiger/main.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "-o", "results",
                "-w", "0",
                "-v", "examples/synthtiger/template.py",
                "SynthTiger",
                "examples/synthwhole/config.yaml",
                "--count", "10"
            ],
            "purpose": ["debug-in-terminal"]
          },
          {
            "name": "gen2",
            "type": "python",
            "request": "launch",
            "program": "synthtiger/main.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "-o", "results",
                "-w", "0",
                "-v", "examples/synthwhole/template.py",
                "SynthWhole",
                "examples/synthwhole/config.yaml",
                "--count", "10"
            ],
            "purpose": ["debug-in-terminal"]
          }
       
    ]
